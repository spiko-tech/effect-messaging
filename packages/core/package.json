{
  "name": "@effect-messaging/core",
  "version": "0.2.22",
  "type": "module",
  "license": "MIT",
  "description": "Message broker abstractions for Effect",
  "homepage": "https://github.com/spiko-tech/effect-messaging",
  "repository": {
    "type": "git",
    "url": "https://github.com/spiko-tech/effect-messaging.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/spiko-tech/effect-messaging/issues"
  },
  "tags": [
    "typescript",
    "publisher",
    "subscriber",
    "pubsub",
    "messaging"
  ],
  "keywords": [
    "typescript",
    "publisher",
    "subscriber",
    "pubsub",
    "messaging"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "provenance": true
  },
  "scripts": {
    "codegen": "build-utils prepare-v2",
    "build": "pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v2",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "check": "tsc -b tsconfig.json",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "devDependencies": {
    "effect": "^3.18.2"
  },
  "peerDependencies": {
    "effect": "^3.18.2"
  }
}
